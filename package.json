{
  "name": "redux-form-with-ajv",
  "version": "0.1.2",
  "description": "validate your redux forms with ajv (json-schema)",
  "scripts": {
    "build": "babel src --out-dir ./ --ignore **/*.spec.js",
    "lint": "eslint src",
    "test": "nyc --reporter=html --reporter=text mocha ./src/index.spec.js --compilers js:babel-core/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "examples:simple": "node webpack.server.dev.js ./examples/simple/webpack.config.js",
    "examples:fieldArrays": "node webpack.server.dev.js ./examples/fieldArrays/webpack.config.js"
  },
  "main": "index.js",
  "author": "Jan Salloth aka < yuki ^.^ >",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js"
  ],
  "repository": {
    "type" : "git",
    "url" : "https://github.com/gitjs/redux-form-with-ajv"
  },
  "keywords": [
    "validation",
    "redux-form",
    "ajv"
  ],
  "dependencies": {
    "ajv": "^5.2.2",
    "ajv-errors": "^1.0.0",
    "object-path": "^0.11.4"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "cross-env": "^3.1.3",
    "eslint": "^3.11.1",
    "eslint-config-rackt": "1.1.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.7.1",
    "eventsource-polyfill": "0.9.6",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-loader": "^0.4.4",
    "html-webpack-include-assets-plugin": "0.0.4",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "0.5.4",
    "markdown-loader": "0.1.7",
    "mocha": "^3.2.0",
    "nyc": "^11.1.0",
    "raw-loader": "0.5.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-intl": "^2.2.3",
    "react-redux": "^5.0.3",
    "redbox-react": "^1.3.3",
    "redux": "^3.6.0",
    "redux-form": "^7.0.3",
    "redux-form-website-template": "0.0.41",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.3",
    "webpack-bundle-size-analyzer": "^2.6.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  }
}
